package com.xpp.moblie.print;

import java.util.List;

public class OrderDetail {
	private String skuId;// 品项
	private String sku;
	private double quantity;
	private double price;
	private double totalPrice;
	private String orderType;// A本品B赠品;
	private String orderTypeTxt;// 本品,赠品;
	private String unitCode; // 数量单位编码
	private String unitDesc;// 数量单位
	private String priceUnitDesc;// 单价单位
	private String mappingSKUId;// 关联行项目 skuId&unitCode
	private List<OrderDetail> childSkus;// 本品搭赠的赠品sku

	public String getSku() {
		return this.sku;
	}

	public void setSku(String sku) {
		this.sku = sku;
	}

	public double getQuantity() {
		return this.quantity;
	}

	public void setQuantity(double quantity) {
		this.quantity = quantity;
	}

	public double getPrice() {
		return this.price;
	}

	public void setPrice(double price) {
		this.price = price;
	}

	public double getTotalPrice() {
		return this.totalPrice;
	}

	public void setTotalPrice(double totalPrice) {
		this.totalPrice = totalPrice;
	}

	public String getOrderType() {
		return orderType;
	}

	public void setOrderType(String orderType) {
		this.orderType = orderType;
	}

	public String getOrderTypeTxt() {
		return orderTypeTxt;
	}

	public void setOrderTypeTxt(String orderTypeTxt) {
		this.orderTypeTxt = orderTypeTxt;
	}

	public String getUnitCode() {
		return unitCode;
	}

	public void setUnitCode(String unitCode) {
		this.unitCode = unitCode;
	}

	public String getUnitDesc() {
		return unitDesc;
	}

	public void setUnitDesc(String unitDesc) {
		this.unitDesc = unitDesc;
	}

	public String getPriceUnitDesc() {
		return priceUnitDesc;
	}

	public void setPriceUnitDesc(String priceUnitDesc) {
		this.priceUnitDesc = priceUnitDesc;
	}

	public String getMappingSKUId() {
		return mappingSKUId;
	}

	public void setMappingSKUId(String mappingSKUId) {
		this.mappingSKUId = mappingSKUId;
	}

	public List<OrderDetail> getChildSkus() {
		return childSkus;
	}

	public void setChildSkus(List<OrderDetail> childSkus) {
		this.childSkus = childSkus;
	}

	public String getSkuId() {
		return skuId;
	}

	public void setSkuId(String skuId) {
		this.skuId = skuId;
	}

}